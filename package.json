{
  "name": "obsidian-sql-workbench",
  "version": "1.0.0",
  "description": "SQL Workbench for Obsidian: SQL editing, formatting, and commenting helpers.",
  "main": "main.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint ."
  },
  "keywords": [
    "obsidian",
    "plugin",
    "sql",
    "formatter",
    "codemirror"
  ],
  "license": "0-BSD",
  "devDependencies": {
    "@codemirror/commands": "^6.10.1",
    "@codemirror/lang-sql": "^6.10.0",
    "@codemirror/language": "^6.12.1",
    "@codemirror/state": "^6.5.0",
    "@codemirror/view": "^6.38.6",
    "@eslint/js": "9.30.1",
    "@types/mssql": "^9.1.8",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.16.0",
    "esbuild": "0.25.5",
    "eslint-plugin-obsidianmd": "0.1.9",
    "globals": "14.0.0",
    "jiti": "2.6.1",
    "obsidian": "1.10.3",
    "tslib": "2.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.35.1"
  },
  "dependencies": {
    "mssql": "^12.2.0",
    "mysql2": "^3.16.0",
    "pg": "^8.16.3",
    "promise": "^8.3.0",
    "sql-formatter": "^15.6.12",
    "ts-morph": "^27.0.2"
  }
}
